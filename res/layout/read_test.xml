<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:scrollbars="vertical">
    <LinearLayout
        android:layout_width="fill_parent"
        android:orientation="vertical"
        android:layout_height="fill_parent">
        <TextView
            android:id="@+id/setup_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Read Test"
            android:textSize="12pt">
        </TextView>
        <TextView
            android:id="@+id/read_val_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:textSize="10pt">
        </TextView>
        <TextView
            android:id="@+id/read_val_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:textSize="10pt">
        </TextView>
        <TextView
            android:id="@+id/read_val_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:textSize="10pt">
        </TextView>
        <TextView
            android:id="@+id/read_val_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:textSize="10pt">
        </TextView>
        <TextView
            android:id="@+id/file_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:textSize="10pt">
        </TextView>
        <TextView
            android:id="@+id/parse_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:textSize="10pt">
        </TextView>
        <Button
            android:id="@+id/ButtonSendFeedback"
            android:layout_height="wrap_content"
            android:text="back to project setup"
            android:onClick="project_setup"
            android:layout_width="fill_parent">
        </Button>
    </LinearLayout>
</ScrollView>
